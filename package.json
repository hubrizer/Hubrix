{
  "name": "hubrix-wp-plugin-framework",
  "version": "1.0.0",
  "description": "Hubrix - modern WordPress plugin Framework",
  "scripts": {
    "build:frontend-dev": "webpack --config webpack.frontend.js",
    "build:backend-dev": "webpack --config webpack.backend.js",
    "build:frontend-prod": "cross-env NODE_ENV=production webpack --config webpack.frontend.js",
    "build:backend-prod": "cross-env NODE_ENV=production webpack --config webpack.backend.js",
    "build": "npm run build:frontend-prod && npm run build:backend-prod",
    "build:prod": "npm run build:frontend-prod && npm run build:backend-prod",
    "build:dev": "npm run build:frontend-dev && npm run build:backend-dev",
    "start:frontend": "cross-env NODE_ENV=development webpack serve --config webpack.frontend.js --open",
    "start:backend": "cross-env NODE_ENV=development webpack serve --config webpack.backend.js --open"
  },
  "keywords": [],
  "author": "Hubrizer Team",
  "license": "GPLv3",
